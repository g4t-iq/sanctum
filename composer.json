{
    "name": "tune-framework/sanctum",
    "description": "tune Sanctum provides a featherweight authentication system for SPAs and simple APIs.",
    "keywords": ["tune", "sanctum", "auth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/tune/sanctum/issues",
        "source": "https://github.com/tune/sanctum"
    },
    "authors": [
        {
            "name": "Hussein Alaa",
            "email": "hussein@tune-framework.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-json": "*",
        "g4t/console": "^9.21|^10.0",
        "g4t/contracts": "^9.21|^10.0",
        "g4t/database": "^9.21|^10.0",
        "g4t/support": "^9.21|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^7.28.2|^8.8.3",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Tune\\Sanctum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tune\\Sanctum\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "tune": {
            "providers": [
                "Tune\\Sanctum\\SanctumServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
